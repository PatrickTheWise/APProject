      </MenuButton>
      <Label layoutX="26.0" layoutY="70.0" text="swap" textFill="#eaeaea">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <MenuButton fx:id="menuto" layoutX="26.0" layoutY="313.0" mnemonicParsing="false" onAction="#tomenu" text="TO">
        <items>
          <MenuItem fx:id="GBPto" mnemonicParsing="false" onAction="#toGBP" text="GBP" />
          <MenuItem fx:id="YENto" mnemonicParsing="false" onAction="#toYEN" text="YEN" />
            <MenuItem fx:id="TOMANto" mnemonicParsing="false" onAction="#toTOMAN" text="TOMAN" />
            <MenuItem fx:id="USDto" mnemonicParsing="false" onAction="#toUSD" text="USD" />
            <MenuItem fx:id="EURto" mnemonicParsing="false" onAction="#toEUR" text="EUR" />
        </items>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </MenuButton>
      <Label layoutX="26.0" layoutY="276.0" text="to" textFill="#eaeaea">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="vorrodi" layoutX="136.0" layoutY="116.0" prefHeight="26.0" prefWidth="150.0">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></TextField>
      <ToolBar layoutX="178.0" layoutY="421.0" prefHeight="66.0" prefWidth="244.0" style="-fx-background-color: #ad974f;">
         <items>
            <Button fx:id="home" minWidth="50.0" mnemonicParsing="false" onAction="#tohome" prefHeight="22.0" prefWidth="40.0">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@home.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="transfer" mnemonicParsing="false" onAction="#totransfer">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@transfer.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="swap" alignment="CENTER" minWidth="41.0" mnemonicParsing="false" onAction="#toswap" prefHeight="48.0" textOverrun="CENTER_WORD_ELLIPSIS">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@swap.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="prof" mnemonicParsing="false" onAction="#toprof" prefWidth="50.0">
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@profile.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </items>
