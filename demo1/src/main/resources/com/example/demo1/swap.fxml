          <MenuItem fx:id="GBPswap" mnemonicParsing="false" onAction="#swapGBP" text="GBP" />
          <MenuItem fx:id="YENswap" mnemonicParsing="false" onAction="#swapYEN" text="YEN" />
            <MenuItem fx:id="TOMANswap" mnemonicParsing="false" onAction="#swapTOMAN" text="TOMAN" />
            <MenuItem fx:id="USDswap" mnemonicParsing="false" onAction="#swapUSD" text="USD" />
